@article{wilmerStagingDissensusBaltic2010,
  title = {Staging Dissensus: The {{Baltic}} Way as a Performative Act to Reframe the Geopolitical Sphere},
  author = {Wilmer, Stephen},
  date = {2010},
  journaltitle = {Meno istorija ir kritika},
  number = {6},
  pages = {122--126},
  abstract = {Jacques Ranciere discusses disapprovals (“Dissensus”) as “differences in opinion” within the limits of common sense: discussions about what is given, about the system where we perceive something as given”. The article analyses the Baltic Way of August 1989 as an example of political disapproval, when “two worlds find themselves in one world”. On 23 August 1989, a 600 km human chain connected Tallinn, Riga and Vilnius in order to condemn the Pact of Molotov-Ribbentrop on its 50th anniversary. It was a symbolic event uniting three Soviet republics and an act of protest against the occupation of three independent states in 1939 and in favour of the right to the restoration of independence. As an expression of national and international solidarity, the Baltic Way challenged the Soviet interpretation of history, since it contradicted the statement that the Baltic States joined the Soviet Union on their own free will. Almost 2 million participants of this campaign clearly showed the reality of mass resistance against the Soviet authorities. Moreover, this event drew a new political and geographical map, highlighting national borders between the Baltic States, which were considered as administrative borders in the Soviet period, not as political units. At the same time it created a physical link uniting the three Baltic States. By disputing the geopolitical system, the Baltic Way clearly demonstrated disapproval thus elevating the right to national citizenship and independence, which had been neglected over 50 years, and predicting the collapse of the Soviet Union and the Iron Curtain.},
  keywords = {20 amžius,Daivis Ivans,Estonia,International relations,Jacques Ranciere,Latvia,Lithuania,Vilnius,Vytautas Landsbergis},
  file = {/Users/egorshe/.yandex.disk/90363934/Yandex.Disk.localized/Thesis/Baltic/Cultural Policy/STAGING DISSENSUS, THE BALTIC WAY AS A PERFORMATIVE ACT TO REFRAME THE GEO-POLITICAL SPHERE-- WILMER.pdf}
