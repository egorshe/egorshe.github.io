@article{marshConceptsGenderCitizenship2013,
  title = {The {{Concepts}} of {{Gender}}, {{Citizenship}}, and {{Empire}} and {{Their Reflection}} in {{Post}}‐{{Soviet Culture}}},
  author = {Marsh, Rosalind},
  date = {2013-04},
  journaltitle = {The Russian Review},
