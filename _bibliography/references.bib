@article{janickaWhoCanSpeak2020,
  title = {Who {{Can Speak}}? {{Rancière}}, {{Latour}} and the {{Question}} of {{Articulation}}},
  shorttitle = {Who {{Can Speak}}?},
  author = {Janicka, Iwona},
