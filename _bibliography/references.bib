@article{edensorReifiedMonumentsCounter2023,
  title = {Reified Monuments, Counter Memorials and Anti-Memorials: Contested Colonial Heritage in {{Melbourne}} – Commemorating {{John Batman}}},
  shorttitle = {Reified Monuments, Counter Memorials and Anti-Memorials},
  author = {Edensor, Tim and Sumartojo, Shanti},
  date = {2023-10-02},
  journaltitle = {Postcolonial Studies},
  shortjournal = {Postcolonial Studies},
  volume = {26},
