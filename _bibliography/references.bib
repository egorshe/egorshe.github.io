@article{davoliuteRusticTurnLate2023a,
  title = {The Rustic Turn during Late Socialism and the Popular Movement against {{Soviet}} Rule},
  author = {Davoliūtė, Violeta and Rudling, Odeta},
  date = {2023-01-02},
  journaltitle = {Canadian Slavonic Papers},
  shortjournal = {Canadian Slavonic Papers},
  volume = {65},
