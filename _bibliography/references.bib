  file = {/Users/egorshe/.yandex.disk/90363934/Yandex.Disk.localized/Thesis/Baltic/Cinema/Latvia/the-creative-treatment-of-actuality-poetics-and-verisimilitude-in-laila-pakalnina-s-films.pdf}
