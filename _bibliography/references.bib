@article{ermolaevaSovietLegaciesRussian2024,
  title = {Soviet {{Legacies}} in {{Russian}} ({{B}})Order-{{Making}} and ({{B}})Order-{{Crossing}}},
  author = {Ermolaeva, Oksana},
  date = {2024-12-19},
  journaltitle = {Borders in Globalization Review},
