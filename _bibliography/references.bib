  title = {Constructing {{National Identity}} in {{Soviet Estonian Documentary Cinema}}: {{A Case Study}} of the {{Documentary Ruhnu}} (1965) by {{Andres Sööt}}},
  shorttitle = {Constructing {{National Identity}} in {{Soviet Estonian Documentary Cinema}}},
  author = {Västrik, Riho},
  date = {2015-11-01},
  journaltitle = {Baltic Screen Media Review},
  volume = {3},
