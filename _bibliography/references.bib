@article{tchernevaLinfluenceVertovDocumentaristes2018,
  title = {L’influence de {{Vertov}} Sur Les Documentaristes Lettons Des Années 1960 : Un Projet d’organisation Collective Du Filmage En Héritage},
  shorttitle = {L’influence de {{Vertov}} Sur Les Documentaristes Lettons Des Années 1960},
  author = {Tcherneva, Irina},
  date = {2018},
  journaltitle = {1895. Mille huit cent quatre-vingt-quinze},
  shortjournal = {1895},
  volume = {85},
  pages = {72--99},
  url = {https://journals.openedition.org/1895/6469},
  urldate = {2025-02-24},
  abstract = {Cette étude examine le rôle qu’ont joué les écrits de Vertov dans les années 1960, au moment même où on les redécouvrait en URSS, dans un studio bien précis du pays, celui de Riga en Lettonie et sur l’impulsion de deux cinéastes en particulier, Uldis Brauns et Gerceļs Franks. La Lettonie devenue république socialiste soviétique après la Deuxième Guerre seulement n’avait pas eu de rapport avec les mouvements et les débats qui avaient agité la scène cinématographique soviétique dans les années 1920-1930. En exploitant des fonds d’archives inédits, l’article montre comment Brauns et Franks recoururent à l’enseignement de Vertov dans le cadre d’une commande spécifique qui leur avait été passée~: la réalisation d’un film commémoratif pour le 50e anniversaire de la Révolution d’Octobre. Ce film, 235~millions de visages, tirait profit de l’exemple de la 6e Partie du monde mais surtout il était l’occasion de re-poser à nouveaux frais les questions d’organisation de la production d’un film documentaire collectif de cette envergure~: renonciation au scénario, organisation collective coordonnée et~souple, exploration du comportement humain par la caméra. Depuis les années 1930 l’industrie du cinéma en URSS s’était formée à l’opposé du projet vertovien, en devenant une institution concentrée, mais présente dans les territoires, fondée sur un investissement permanent de cinéastes professionnels.           ,              This study examines the role played by Vertov’s writings in the 1960s, at the very moment when they were rediscovered in the USSR, in a very specific studio in the country, that of Riga in Latvia, and driven by two filmmakers, in particular, Uldis Brauns and Gerceļs Franks. Latvia, which became a Soviet Socialist Republic after the Second World War, had little relationship with the movements and debates that had agitated the Soviet film scene in the 1920s and 1930s. By exploring unpublished archival material, this article shows how Brauns and Franks turned to Vertov’s teaching for a specific film that they had been commissioned to make~: a commemorative film for the 50th anniversary of the October Revolution. This film, 235 Million Faces, drew on the example of The 6th Part of the World, but especially it was the opportunity to re-examine key questions of organization of the production of a collective documentary film of this scale~: rejection of the scenario, coordinated and flexible collective organization, exploration of human behaviour by the camera. Since the 1930s the film industry in the USSR had evolved in opposition to the Vertovian project, becoming a concentrated institution, but present in the territories, based on a permanent deployment of professional filmmakers.           ,              Lo studio è dedicato al ruolo ricoperto dagli scritti di Vertov negli anni Sessanta, ovvero quando venivano riscoperti in URSS e in particolare negli studi di Riga, in Lettonia, grazie al contributo di due registi, Uldis Brauns e Gerceļs Franks. La Lettonia, divenuta repubblica socialista sovietica solo dopo la seconda guerra mondiale, non era stata sfiorata dai movimenti e dalle discussioni che avevano percorso la scena cinematografica sovietica negli anni Venti e Trenta. Sfruttando fonti archivistiche inedite l’articolo dimostra come Brauns e Frank facciano riferimento al magistero di Vertov nel quadro di una commissione che era stata affidata loro, ovvero la realizzazione di un film commemorativo per il 50o~anniversario della Rivoluzione d’Ottobre. il film, 235 milioni di volti, si ispirava a La sesta parte del mondo, ma fu soprattutto l’occasione di porre di nuovo in discussione l’organizzazione della produzione di un film documentario collettivo di questa portata~: rifiuto della sceneggiatura, organizzazione collettiva coordinata e agile, esplorazione del comportamento umano grazie alla macchina da presa. Dopo gli anni Trenta, l’industria sovietica del cinema si era mossa in una direzione opposta a quella del progetto vertoviano, divenendo un’istituzione centripeta ma presente sul territorio e fondata su un investimento permanente di registi professionisti.},
  file = {/Users/egorshe/Zotero/storage/L6P4E987/Tcherneva - 2018 - L’influence de Vertov sur les documentaristes lettons des années 1960  un projet d’organisation col.pdf}
}

