@article{rangoyTrainLanguageTrain2021,
  title = {Train of {{Language}}, {{Train}} of {{Thought}}: {{Notes}} on an {{Exophonic Anomaly}}},
  shorttitle = {Train of {{Language}}, {{Train}} of {{Thought}}},
