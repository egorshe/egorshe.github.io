@article{veststeynKakNazvatGovoryashchego2019,
  title = {Как назвать “говорящего” в стихотворении? (“поэт”, “лирический субъект”, “лирическое ‘Я’”, “лирический герой” и другие термины, применяемые в анализе лирической поэзии)},
  shorttitle = {Как назвать “говорящего” в стихотворении?},
  author = {Вестстейн, Виллем Г.},
  date = {2019-11},
  journaltitle = {Russian Literature},
  shortjournal = {Russian Literature},
  volume = {109--110},
  pages = {31--46},
  issn = {03043479},
  doi = {10.1016/j.ruslit.2019.11.002},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0304347919300535},
  urldate = {2025-04-28},
  abstract = {In nineteenth-century Romantic poetry, the lyric “I” was usually identified with the poet himself. This changed at the beginning of the twentieth century, when poets (Pound and Eliot, for instance) introduced speakers (“personae”) into their poetry who were clearly different from themselves. Instead of “the poet”, new terms were introduced to refer to the speaker in the poem. In German lyric theory, the term “das lyrische Ich” (“the lyric I”) became popular, as in Russian did “liricheskii geroi” (“the lyric hero”). English and American critics generally use “the speaker” and “the poet” (often indiscriminately in the same book or article). As the speaker in the poem is, in principle, a fictive construct and cannot be identified with the poet, and as many poems do not have an “I”, this paper argues for the use of the term “the speaker”.},
  langid = {russian},
  file = {/Users/egorshe/.yandex.disk/90363934/Yandex.Disk.localized/Thesis/Linguistics, Literature, Word/Russian Literature 109–110 (2019) СУБЪЕКТ/КАК НАЗВАТЬ “ГОВОРЯЩЕГО” В СТИХОТВОРЕНИИ?.pdf}
}

