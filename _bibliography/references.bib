@article{kovacevicDissolvingSeaCinematic2019,
  title = {Dissolving into the Sea: Cinematic Migrants and the Problem of Agency},
  shorttitle = {Dissolving into the Sea},
  author = {Kovačević, Nataša},
  date = {2019-10-02},
  journaltitle = {Postcolonial Studies},
