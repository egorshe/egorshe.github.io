@article{strupuleLATVIANAMATEURDOCUMENTARY2022,
  title = {{{LATVIAN AMATEUR DOCUMENTARY FILM}}, 1970s–1980s: {{FAMILY}}, {{COMMUNITY}}, {{TRAVEL}}, {{AND POLITICS IN THE FILMS OF ULDIS LAPIŅŠ}}, {{INGVARS LEITIS}}, {{AND ZIGURDS VIDIŅŠ}}},
