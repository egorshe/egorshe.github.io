  shorttitle = {On {{Tracks}}, {{Facts}}, and the {{Living Man}}. {{Sergei Tret}}’iakov’s {{Implied Ethnographic Turn}} (Or},
  author = {Wurm, Barbara},
  date = {2019-01},
  journaltitle = {Russian Literature},
