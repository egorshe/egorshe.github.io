@article{eglaja-kristsoneExileLiminalityExperience2015,
  title = {Exile and {{Liminality}}: {{Experience}} between {{Cultures}} and {{Identities}}},
  shorttitle = {Exile and {{Liminality}}},
  author = {Eglāja-Kristsone, Eva},
  date = {2015-07-07},
  journaltitle = {Interlitteraria},
  volume = {20},
