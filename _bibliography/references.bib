@article{zukauskieneKinasIrDaugialypis2015,
  title = {Kinas ir daugialypis atminties diskursas},
  author = {Žukauskienė, Odeta},
  date = {2015},
  journaltitle = {Lietuvos kultūros tyrimai},
  shortjournal = {LKT},
  volume = {6},
  pages = {36--57},
  issn = {20298560},
  doi = {10.53630/lkt.2015_1.2},
  url = {https://www.sphairos.lt/wp-content/uploads/2021/05/LKT-6__2015__Odeta.Zukauskiene.Kinas_.ir_.daugialypis.atminties.diskursas.pdf},
  urldate = {2025-04-29},
  abstract = {This article discusses the ambivalent discourses of cinematography and memory. It aims to show that a deeper exploration of memory is related to the development of cinema, which actualizes memory-as-image. Visualizing realities that are exposed to individuals as the remembering, cinema has become a part of memory technologies, which gives meaning to the emotional memory and at the same time plunges memory into the ocean of controversial visual culture, meaninglessness and oblivion. As the form of prosthetic memory and the extension of human consciousness, cine ma began to reflect on the memory and encouraged the development of memory discourse in the twentieth century. It contributed to the merging of collective memory into individual memories, to the replacing “history” with “memory” and grand narrative with many small, local layers of remembrance, to the revival of oral history and simple human experience. This opening of cinematic perspective, that weaves past and present, established new forms and practices of cultural memory. It can be observed that the profound impact of cinema on the collective imagination and memory drew attention to the national cinematography as an important tool to mobilize citizens, activate communities, reflect, reinforce and transform identity structures. Lithuanian cinematic culture survived the crisis at the intersection of XX–XXI c. and also began to reflect on the processes of remembering and recall. The disclosure of traumatic, repressed, false memory and its creative conversion into long-term post-memory become one of the major tasks of cinema as a medium of cultural memory. Many different motifs of memory can be seen in documentaries as well as feature films.},
  langid = {lithuanian},
  file = {/Users/egorshe/.yandex.disk/90363934/Yandex.Disk.localized/Thesis/Baltic/Cinema/Lithuania/Cinema and the multifaceted discourse of memory.pdf}
}

@thesis{zurkuvieneFilmHistoricalSource2008,
  type = {mathesis},
  title = {Film as Historical Source the Case of Communism},
  author = {Žurkuvienė, Ina},
  date = {2008},
  institution = {Vytautas Magnus University},
  abstract = {The choice of the topic of this paper was inspired by the search of new directions of research and methods in political sciences. Film as the source of history still has no acknowledgment in many countries and especially in Lithuania: there are no scientific texts (of political studies) here, but references to political context of film can be found in individual works of Lithuanian scholars (mostly in discussions on Soviet film). New method of interpretation was chosen for the analysis of the forms of communism – one of the most researched issues in the world. The difficulties that were faced during the search for literature in Lithuanian as well as other countries were mostly determined by the novelty of the topic itself. Not many authors have seen the possibilities of film as a source of information. This work discusses all major texts of these theorists: John E. O‘Connor, Pierre Sorlin and Marc Ferro. Their insights have provided a robust foundation for further analysis. Meanwhile, the philosophical reflections of Slavoj Žižek, that have revived the traditional philosophy by incorporating it into the popular culture, i.e. reality, allowed not only to penetrate the nature of complexity of the film position among the scholars, which was influenced by the tight narrative of medial culture studies, but also provided reasons and solutions of this problem. The goal of the study is to determine the possibilities of film as the source of information. Two hypotheses are formulated as the base for the analysis:  Film can be justly incorporated in the sphere of academic studies as a tool/source of research.  The nature of historical-political film may be either neorealistic, or as a form of aesthetisation. This paper includes a qualitative study. The concept of an historical film as the main carrier of the film genre and its features shall play the key role in this analysis. It differentiates, provides the key units for the study and even draws a certain scheme for the treatment of political film in social research. The last statement predetermines the peculiarity of the analysis – film as an historical source. The first, theoretical, part shall identify two issues related to the film theory – lack of organised methodology and the tendency of the definition of the historical film itself towards ambivalence. The following article describes possible methods of film analysis that are important for the interpretation of selected movies. Discussing of the definition of an historical-political film is not enough to validate the first hypothesis – in such case, the position of cinematography in the scale of the new methods of research as well as political sciences (in terms of search for new directions) would still be uncertain. One of the parts of the paper includes a philosophical study and looks at the film as the philosophy and new directions. The process of search is based on nonprobability sampling of filmography. Samples were chosen in accordance with the goal and according to the Sorlin’s definition. The following additional criteria were defined: period, political implication and artistic value. Vytautas Žalakevičius, Almantas Grikevičius, Raimondas Vabalas, Eldar Ryazanov, Luchino Visconti, Bernardo Bertolucci and Hans Peter Molland were the filmmakers with a delicate creative sense that enriched their works with originality in terms of political implication as well as artistic style. All works of the directors reviewed here (“Nobody Wanted to Die”, “One Day Chronicle”, “Feelings”, “Ladder to the Sky”, “Watch out for the Automobile”, “Rocco and His Brothers”, “1900”, “The Dreamers” and “Comrade Pedersen”) are related to the same period of time – 1960s and 1970s – and have been ‘marked’ by it. The filmmakers who associated this period with the appearance of new ideas, artistic uplift and the coming of historical changes represent the period in question with a detachment or through direct contact. Some of the works (“Nobody Wanted to Die”, “One Day Chronicle”, “Feelings”, “Ladder to the Sky”, “Watch out for the Automobile”, “Rocco and His Brothers”) reflect and interpret the present (of the past), while the others (“1900”, “The Dreamers” and “Comrade Pedersen”) reconstruct the past. The choice between these two narrative lines was predetermined by the pretension to the historical reality and the aim to introduce adequate analysis of the past. Therefore, both the subsistence (the reality of the present moment, perception, which has a tendency to change in time) and the ability of the author to reproduce historical events with ‘detachment’, after a period of time, during which unknown details of the past could have been revealed.},
  file = {/Users/egorshe/.yandex.disk/90363934/Yandex.Disk.localized/Thesis/Media/Film as historical source the case of communism.pdf}
}

@article{zvirgzdasHenrikasNagysMediator2015,
  title = {Henrikas {{Nagys}} as a {{Mediator}} of {{Lithuanian}} and {{Latvian Poetical Traditions}}},
  author = {Žvirgždas, Manfredas},
  date = {2015-07-07},
  journaltitle = {Interlitteraria},
  volume = {20},
  number = {1},
  pages = {94},
  issn = {2228-4729, 1406-0701},
  doi = {10.12697/IL.2015.20.1.8},
  url = {http://ojs.utlib.ee/index.php/IL/article/view/IL.2015.20.1.8},
  urldate = {2023-07-19},
  abstract = {In this article, motifs, references and influences of Latvian descent in the poetry of Lithuanian Henrikas Nagys (1920–1996) who spent most of his creative life in Canada are observed and analyzed. Nagys was praised by critics as one of the main modernizers of the national poetical vocabulary; nevertheless he was regarded as one of the most emotionally suggestive and ideologically engaged poets of Lithuanian Western exile. He belonged to the Žemininkai movement which played an important role transforming lyrical tradition in the diaspora during the 1950s; the members of this group were deeply influenced by the post-Naturalist trends of Western Modernism and Existentialist philosophy; they were members of the generation which got educated in the gymnasiums of independent Lithuania. Nagys used to translate contemporary Latvian authors who had the similar experience and who adopted the mythic Baltic heritage but at the same time reflected some kind of Protestant simplicity and sobriety, natural vitalism and economy of expression. Nagys promoted Latvian poetical tradition introducing Velta Sniķere, Gunars Saliņš, Aina Kraujiete, Aina Zemdega, and Astrīde Ivaska in his Lithuanian translations. Simplified Latvian poetical style with its free versification, pure images and folklore-based models of spatial and chronological organization was reflected in Nagys’ later poems. His links with Latvia were biographical, based on childhood memories; however his imaginary landscape was a result of fusion of universal Northern features. Nagys popularized the symbolism of the North, and Latvian place-names were included into the map of his identity.},
  langid = {english},
  file = {/Users/egorshe/Zotero/storage/GHMMK9RC/Žvirgždas - 2015 - Henrikas Nagys as a Mediator of Lithuanian and Lat.pdf}
}
