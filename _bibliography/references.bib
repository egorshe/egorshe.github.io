@article{hackmannNorthEastSomething2025,
  title = {North, {{East}} or {{Something Else}}? {{Locating}} the {{Baltic Sea Region}} on the {{Mental Maps}} of {{Europe}}},
  shorttitle = {North, {{East}} or {{Something Else}}?},
  author = {Hackmann, Jörg},
  date = {2025},
  journaltitle = {Nordiques},
  shortjournal = {nordiques},
  volume = {47},
  issn = {1761-7677, 2777-8479},
  doi = {10.4000/1432n},
  url = {https://journals.openedition.org/nordiques/11827},
  urldate = {2025-11-13},
  abstract = {Il existe de nombreux concepts géographiques et historiques pour définir l’Europe du Nord, l’Europe de l’Est ainsi que la région de la mer Baltique. Les études sur les cartes mentales ont déjà démontré que ces notions ne sont pas uniquement fondées sur la géographie physique ou sur des réalités historiques clairement délimitées. Au contraire, les concepts se chevauchent souvent, sont aussi parfois partiellement contradictoires~; ils diffèrent selon les langues et changent avec le temps. Ces divisions spatiales de l’Europe (comme d’au-delà) proviennent en premier lieu des imaginaires politiques, sociaux et culturels. Elles s’enchevêtrent lorsqu’on les observe à l’échelle transnationale, comme l’immense majorité des phénomènes socio-culturels, et dépendent de paramètres politiques et socio-culturels, mais aussi des époques et des points de vue des divers acteurs. Cet article s’intéresse aux concepts épistémologiques et politiques liés aux espaces baltiques et nordiques ainsi qu’à leur évolution, surtout depuis le XIXe~siècle. Il se penche tout d’abord sur les notions mouvantes de «~Nord~» et d’«~Est~» de l’Europe, puis de région «~baltique~». Dans les sections suivantes, la fluidité de l’espace «~baltique~» est présentée et discutée au prisme du concept d’espace «~nordique~». De plus, d’autres concepts culturels, politiques ou académiques, qui font référence à une région baltique étendue, comme par exemple la «~Baltoscandie~» ou l’Europe du Nord-Est, sont examinés. L’article met en évidence le fait que, d’une part, les différents concepts d’espace «~baltique~» ne font pas uniquement référence aux «~trois États baltes~» et que, d’autre part, les notions de «~baltique~» et de «~nordique~» se recoupent et se mélangent au XXe et au XXIe~siècles. Enfin, dans les discussions actuelles, l’espace «~baltique~», à la différence du «~Norden~», n’est pas forcément fondé sur une homogénéité culturelle, sociale ou politique, mais suppose plutôt un intérêt particulier pour la question de l’unité compte tenu de la diversité des sociétés qui peuplent la région de la mer Baltique.           ,              There are various spatial and historical concepts of Northern and Eastern Europe, and~– not least~– of the Baltic Sea Region. Debates on mental maps have already shown that such notions are neither based solely on physical geography nor on clearly delimitated historical units. Instead, the concepts are often overlapping, and partly also contrary to each other, they are differing according to languages, and in addition changing over time. Thus, such debates about the spatial division of Europe (and beyond) emerge first of all from political, social, and cultural imaginaries. Furthermore, they are as almost every social and cultural phenomenon transnationally entangled and depend on political, social, and cultural settings as well as on epochs and perspectives. This text focuses on epistemological and political concepts connected to Baltic and Nordic spaces and their trajectories, in particular since the 19th~century. It starts with the changing notions of, first, the European “North” and “East”, and, second, the “Baltic” region. In the following sections, the fluidity of “Baltic” space is presented and discussed in its relations to concepts of “Nordic” space. Furthermore, other scholarly and political or cultural notions that refer to the wider Baltic Sea region or to parts of it, as for instance Baltoscandia or North-Eastern Europe, are addressed. Main theses developed in this text are, first, that concepts of “Baltic” space do not exclusively refer to the “three Baltic States” and, second, that notions referring to “Baltic” or “Nordic” are closely intertwined in the 20th and 21st~centuries. Third, in recent debates “Baltic” space~– in distinction to “Norden”~– is based less on the region’s cultural, social, or political homogeneity, but encompasses a focus on the issue of unity in diversity within the societies of the Baltic Sea region.},
  file = {/Users/egorshe/.yandex.disk/90363934/Yandex.Disk.localized/Zotero/03 Thesis Baltic/Trinity/Hackmann - 2025 - North, East or Something Else Locating the Baltic Sea Region on the Mental Maps of Europe.pdf}
