  title = {({{Re}})Construction of {{National Identity}} in the {{Lithuanian}} and {{Latvian Literary Press}} during the {{National Revival Period}} in the {{Late}} 20th {{Century}}},
  author = {Jonkutė, Viktorija},
  date = {2016-10-25},
  journaltitle = {Respectus Philologicus},
