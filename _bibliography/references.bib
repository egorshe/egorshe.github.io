@article{rimaite-berziunienePositiveDimensionDeSovietization2025,
  title = {The {{Positive Dimension}} of {{De-Sovietization}}: {{The Visuality}} of {{Post-Soviet Monuments}}},
  shorttitle = {The {{Positive Dimension}} of {{De-Sovietization}}},
  author = {Rimaitė-Beržiūnienė, Viktorija},
  date = {2025-11-04},
  journaltitle = {Heritage},
