@article{verdinsProsePoems1980s,
  title = {Prose {{Poems}} of the 1980s},
  author = {Verdins, Karlis},
  abstract = {This paper explores Latvian prose poetry of the 1980s - the work of a group of poets who used various strategies to overcome the limitations of Soviet literature. While some poets of the younger generation, like Maris Melgalvs and Peters Bruveris, created their own poetical space within the context of folklore or ancient imagery, others, like Amanda Aizpuriete and Klavs Elsbergs, absorbed the traditions of modern literature. Two key contributors to prose poetry in the 1980s were Uldis BerzirB, whose three collections of poems include prose poems questioning various periods of Latvian history, and Imants Ziedonis, with the prose poems in his collection Attack of the Butterflies (1988). At the end of the 1980s it was possible to open up a discussion with the official ideology and to challenge its viewpoints; this can be seen in the prose poems of Guntars GodinL Viktors Livzemnieks and Imants Ziedonis, as well as Uldis Berzins. Prose poems also documented the process of national awakening, becoming much like newspaper columns in their comments on current events and the conflicting ideologies of national self-determination and Soviet imperialism.},
  langid = {english},
  file = {/Users/egorshe/.yandex.disk/90363934/Yandex.Disk.localized/Thesis/Baltic/Poetry/prose poems of the 1980s latvia 0001.pdf}
}

